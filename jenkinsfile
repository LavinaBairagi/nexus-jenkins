
node {
    stage('Preparation') { // for display purposes
        // Get some code from a GitHub repository
        git 'https://github.com/LavinaBairagi/jenkins.git'
        
    }
    stage('Build') {
                bat "mvn package"
    }
    
   
    
    stage('Nexus') {
       nexusArtifactUploader artifacts: [[artifactId: 'nexus-jenkins',
 classifier: '', file: 'target/nexus-jenkins.0.0.1.war',
 type: 'war']], credentialsId: 'nexus', groupId: 'com.jenkins.nexus', 
nexusUrl: 'localhost:8110', nexusVersion: 'nexus3',
 protocol: 'http', 
repository: 'http://localhost:8110/repository/maven-central-repository/',
 version: '0.0.1'    }
}
